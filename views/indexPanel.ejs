<!-- Include Header -->
<%- include back-end/layout/header.ejs %>

<div class="preloader">
    <svg class="circular" viewBox="25 25 50 50">
        <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
    </svg>
</div>
<!-- ============================================================== -->
<!-- Wrapper -->
<!-- ============================================================== -->
<div id="wrapper">
    <!-- ============================================================== -->
    <!-- Topbar header - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <nav class="navbar navbar-default navbar-static-top m-b-0">
        <div class="navbar-header">
            <div class="top-left-part">
                <!-- Logo -->
                <a class="logo" href="/panel">
                    <b></b>
                    <span class="hidden-xs">
                        <img src="../../back-end/plugins/images/panelLogo.png" alt="home" style="width: 200px"
                            class="light-logo" />
                    </span> </a>
            </div>
            <!-- /Logo -->
            <ul class="nav navbar-top-links navbar-right pull-right">
                <li>
                    <form role="search" class="app-search hidden-sm hidden-xs m-r-10">
                        <input type="text" placeholder="Search..." class="form-control"> <a href=""><i
                                class="fa fa-search"></i></a> </form>
                </li>
                <li>
                    <a class="profile-pic" href="#"> <img src="../../back-end/plugins/images/admin.png" alt="user-img"
                            width="36" class="img-circle"><b class="hidden-xs">Admin</b></a>
                </li>
            </ul>
        </div>
    </nav>
    <!-- End Top Navigation -->
    <!-- ============================================================== -->
    <!-- Left Sidebar - style you can find in sidebar.scss  -->
    <!-- ============================================================== -->
    <div class="navbar-default sidebar" role="navigation">
        <div class="sidebar-nav slimscrollsidebar">
            <div class="sidebar-head">
                <h3><span class="fa-fw open-close"><i class="ti-close ti-menu"></i></span> <span
                        class="hide-menu">Navigation</span></h3>
            </div>
            <ul class="nav" id="side-menu">
                <li style="padding: 70px 0 0;">
                    <a href="/panel" class="waves-effect"><i class="fa fa-clock-o fa-fw"
                            aria-hidden="true"></i>Dashboard</a>
                </li>
                <li>
                    <a href="/panel/editBooking" class="waves-effect"><i class="fa fa-user fa-fw"
                            aria-hidden="true"></i>Profile</a>
                </li>
                <li>
                    <a href="/panel/listBooking" class="waves-effect"><i class="fa fa-table fa-fw"
                            aria-hidden="true"></i>Basic Table</a>
                </li>
                <div class="center p-20">
                    <!-- Include Header -->
                    <a href="#" target="#" class="btn btn-danger btn-block waves-effect waves-light">Log Out</a>
                </div>
            </ul>
        </div>

    </div>
    <!-- ============================================================== -->
    <!-- End Left Sidebar -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Page Content -->
    <!-- ============================================================== -->
    <div id="page-wrapper">
        <div class="container-fluid">
            <div class="row bg-title">
                <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                    <h4 class="page-title">Dashboard</h4>
                </div>
                <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                    <ol class="breadcrumb">
                        <li><a href="#">Dashboard</a></li>
                    </ol>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <!-- ============================================================== -->
            <!-- Different data widgets -->
            <!-- ============================================================== -->
            <!-- .row -->
            <div class="row">
                <div class="col-lg-4 col-sm-6 col-xs-12">
                    <div class="white-box analytics-info">
                        <h3 class="box-title">Total Booking</h3>
                        <ul class="list-inline two-part">
                            <li>
                                <div id="sparklinedash"></div>
                            </li>
                            <li class="text-right"><i class="ti-arrow-up text-success"></i> <span
                                    class="counter text-success"><%= totalBooking %></span></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6 col-xs-12">
                    <div class="white-box analytics-info">
                        <h3 class="box-title">Total Page Views</h3>
                        <ul class="list-inline two-part">
                            <li>
                                <div id="sparklinedash2"></div>
                            </li>
                            <li class="text-right"><i class="ti-arrow-up text-purple"></i> <span
                                    class="counter text-purple">869</span></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6 col-xs-12">
                    <div class="white-box analytics-info">
                        <h3 class="box-title">Unique Visitor</h3>
                        <ul class="list-inline two-part">
                            <li>
                                <div id="sparklinedash3"></div>
                            </li>
                            <li class="text-right"><i class="ti-arrow-up text-info"></i> <span
                                    class="counter text-info">911</span></li>
                        </ul>
                    </div>
                </div>
            </div>
            <!--/.row -->
            <!-- ============================================================== -->
            <!-- table -->
            <!-- ============================================================== -->
            <div class="row">
                <div class="col-md-12 col-lg-12 col-sm-12">
                    <div class="white-box">
                        <div class="col-md-3 col-sm-4 col-xs-6 pull-right">
                            <select class="form-control pull-right row b-none">
                                <option>March 2017</option>
                                <option>April 2017</option>
                                <option>May 2017</option>
                                <option>June 2017</option>
                                <option>July 2017</option>
                            </select>
                        </div>
                        <h3 class="box-title">Recent Booking</h3>
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Nama Pemilik</th>
                                        <th>Merk Mobil</th>
                                        <th>Tanggal Service</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (bookingList) { %>
                                    <% bookingList.forEach(function(data){ %>
                                    <tr>
                                        <td><%= data.id%></td>
                                        <td class="txt-oflo"><%= data.namaPemilik %></td>
                                        <td><%= data.merkMobil %></td>
                                        <td class="txt-oflo"><%= data.tanggalService %></td>
                                        <td>
                                            <a href="/panel/showDetails/<%= data.id %>"><button class="btn btn-primary notika-btn-primary">Show Details</button></a>
                                        </td>
                                        <td>
                                            <form action="/panel/deleteDetails/<%= data.id %>" method="POST">
                                                <form method="post" action="/panel/deleteDetails/<%= data.id %>">
                                                    <button type="submit" name="delete" value='Delete'
                                                        onClick="return confirm('Are you sure you want to delete this client booking data?')"
                                                        class="btn btn-danger">Remove
                                                        Data</button>
                                                    <input type="hidden" name="_method" value="DELETE" />
                                                </form>
                                            </form>
                                        </td>
                                        <td>
                                            <a href="#"><button class="btn btn-success">Done</button></a>
                                        </td>
                                    </tr>
                                    <% }) %>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.container-fluid -->
        <footer class="footer text-center"> 2017 &copy; Ample Admin brought to you by wrappixel.com </footer>
    </div>
    <!-- ============================================================== -->
    <!-- End Page Content -->
    <!-- ============================================================== -->
</div>

<!-- Include Footer -->
<%- include back-end/layout/footer.ejs %>